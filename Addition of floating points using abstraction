abstract class Floatsum
{
    abstract void SUM();
}
public class ADD_FLOAT extends Floatsum
{
    public void SUM()
    {
        float x = 1.15f;
        float y = 1.85f;
        float z = x+y;
        System.out.println("The sum of two numbers = "+ z);
    }
    public static void main(String[] args)
    {
        ADD_FLOAT o = new ADD_FLOAT();
        o.SUM();
    }
}
